{
  "name": "@zguillez/z-ssh",
  "version": "0.2.13",
  "description": "Tools for ssh connections",
  "keywords": [
    "ssh",
    "nodejs",
    "zguillez"
  ],
  "repository": "https://github.com/zguillez/z-ssh",
  "license": "MIT",
  "author": {
    "name": "Guillermo de la Iglesia",
    "email": "mail@zguillez.io",
    "url": "https://github.com/zguillez"
  },
  "main": "index.js",
  "scripts": {
    "clean": "find ./ -name .DS_Store -exec rm -f {} \\; && npm run clean:tmp",
    "clean:tmp": "rimraf .temp/*",
    "deploy": "npm publish --access=public",
    "lint": "eslint index.js src/*.js bin/*.js --fix || true",
    "prepare": "npm run lint && npm run test && sort-package-json && bin/prepare.js",
    "test": "jest --coverage",
    "test_": "node ./example/index.js"
  },
  "dependencies": {
    "@zguillez/z-file": "0.6.10",
    "file-type": "16.5.3",
    "minimist": "^1.2.5",
    "node-ssh": "12.0.0",
    "prompt": "1.2.0",
    "replace": "^1.2.1",
    "shelljs": "0.8.5"
  },
  "devDependencies": {
    "@zguillez/z-git": "^0.1.21",
    "@zguillez/z-log": "^0.1.18",
    "@zguillez/z-version": "^2.0.1",
    "colors": "1.4.0",
    "copy": "0.3.2",
    "del": "6.0.0",
    "eslint": "8.0.0",
    "eslint-config-google": "0.14.0",
    "eslint-plugin-html": "6.2.0",
    "jest": "^27.2.5",
    "rimraf": "3.0.2",
    "sort-package-json": "^1.52.0"
  }
}
